<soap:Envelope xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="urn:zimbraMail">
  <soap:Header>
    <context xmlns="urn:zimbra">
      <authToken>#(authToken)</authToken>
      <csrfToken>#(csfrToken)</csrfToken>
    </context>
  </soap:Header>
  <soap:Body>
    <urn:CreateContactRequest>
      <cn>
        
        #(baseTags)
        #(extendedTags)
        #(phoneTags)
        #(addressTags)
      </cn>
    </urn:CreateContactRequest>
  </soap:Body>
</soap:Envelope>




